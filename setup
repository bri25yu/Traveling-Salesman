sudo apt-get install -y wget

# press enter

mkdir gurobi

cd gurobi

wget https://packages.gurobi.com/9.0/gurobi9.0.0_linux64.tar.gz

tar xvfz gurobi9.0.0_linux64.tar.gz

cd ..

export GUROBI_HOME="${HOME}/gurobi/gurobi900/linux64";

export PATH="${PATH}:${GUROBI_HOME}/bin"

export HTTP_PROXY="chabo-125-7.reshall.berkeley.edu:8899" && grbgetkey --http KEY

# press enter

sudo apt-get install -y git

# press enter

git clone https://TOKEN@github.com/bri25yu/Traveling-Salesman.git

# press enter

cd Traveling-Salesman

git checkout without-parth-optimization

sudo apt-get install -y python3-pip

# press enter

pip3 install networkx mip numpy

sudo apt-get install -y tmux

# in tmux

export GUROBI_HOME="${HOME}/gurobi/gurobi900/linux64";

export PATH="${PATH}:${GUROBI_HOME}/bin"

export LD_LIBRARY_PATH="${LD_LIBRARY_PATH}:${GUROBI_HOME}/lib"

git config --global user.name "cs170-bot"

git config --global user.email "cs170-bot@gcp"

export THREAD_COUNT="32" && python3 solver.py --all inputs outputs

# outside

sudo apt-get install -y htop
